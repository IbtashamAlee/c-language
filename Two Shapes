/*DRAW ANY TWO SHAPES using PRINTF ON THE OUTPUT SCREEN AND THEN COPY THE SOURCE CODE AND OUTPUT SCREENS IN ONE WORD FILE .*/

#include<stdio.h>
#include<stdlib.h>
int main()
{

    //THIS IS CAPITAL L.
    printf("This is a capital L");

    printf("\n");
    printf("\n");

    printf("    **\n");
    printf("    **\n");
    printf("    **\n");
    printf("    **\n");
    printf("    **\n");
    printf("    ********\n");

    printf("\n");
    printf("\n");
    printf("\n");
    //this capital N .
    printf("This is a capital N");

    printf("\n");
    printf("\n");

    printf("    **       **\n");
    printf("    ** *     **\n");
    printf("    **   *   **\n");
    printf("    **     * **\n");
    printf("    **      ***\n");
   printf("\n");
   printf("\n");

  //This is a pyramid.
  printf("This is a pyramid");
  printf("\n");
  printf("\n");
  
  
  printf("      *\n");
  printf("     * *\n");
  printf("    *   *\n");  
  printf("   *     *\n");
  printf("  *       *\n");
  printf(" *         *\n");
  printf("*************\n");

    system("pause");
    return 0;

    }
